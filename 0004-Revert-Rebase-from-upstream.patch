-    args = parser.parse_args()
-
-    navmesh_main(
-        args.YAML_INPUT,
-        args.OUTPUT_DIR)
-    configfile_main(
-        args.YAML_INPUT,
-        args.OUTPUT_DIR,
-        args.WORLD_FILE)
